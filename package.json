{
  "name": "setup-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --open --mode development",
    "start": "npx webpack serve --env NODE_ENV=local --env production --progress",
    "build": "webpack --mode production",
    "clean": "rm -rf dist",
    "commit": "npx git-cz",
    "coverage": "jest --config jest.config.js",
    "format": "npm run pretty -- --write",
    "test": "jest",
    "lint": "eslint --ignore-path .gitignore --ext .js --ext .jsx .",
    "pretty": "prettier --ignore-path .gitignore \"**/*.+(js|json|md)\"",
    "check-format": "npm run pretty -- --list-different",
    "precommit": "npm-run-all --parallel lint pretty",
    "validate": "npm-run-all --parallel check-format lint",
    "cy:run": "percy exec -- npx cypress run",
    "e2e:test": "start-server-and-test start http://localhost:9000 cy:run"
  },
  "author": "Lenny Peters <@loonz806>",
  "license": "MIT",
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
    "analytics": "^0.7.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass": "^1.32.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@percy/cypress": "^2.3.4",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.8.1",
    "css-loader": "^5.0.1",
    "cypress": "^6.3.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
